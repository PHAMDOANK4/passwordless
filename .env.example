# Database Configuration
DB_ROOT_PASSWORD=changeme_root_password
DB_PASSWORD=changeme_db_password

# Twilio SMS Configuration (Optional - for SMS OTP)
TWILIO_MESSAGING_SERVICE_SID=your_twilio_messaging_service_sid
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Email Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_email_app_password
MAIL_SMTP_AUTH=true
MAIL_SMTP_STARTTLS_ENABLE=true

# TOTP Configuration
TOTP_ISSUER=your-company.com
TOTP_ISSUER_LABEL=Your Company Name

# WebAuthn Configuration
WEBAUTHN_RPID=your-domain.com
WEBAUTHN_ORIGIN=https://your-domain.com
